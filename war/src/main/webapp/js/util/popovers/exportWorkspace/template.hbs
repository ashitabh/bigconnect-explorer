<div class="exportWorkspacePopover popover top">
  {{#if showTitle}}
  <div class="popover-title">
    {{ title }}
  </div>
  {{/if}}
  <div class="arrow"></div>
  <div class="popover-content">
    <div class="plugin-content"></div>
    {{#if showCancel}}
    <div>
      <button class="btn btn-default btn-raised btn-sm">{{ i18n 'popovers.export_workspace.button.cancel' }}</button>
    </div>
    {{/if}}
  </div>
</div>
